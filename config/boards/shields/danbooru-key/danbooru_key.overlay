#include "danbooru_key-layouts.dtsi"

/ {
  chosen {
    zmk,kscan = &default_kscan;
    zmk,physical-layout = &physical_layout0;
  };

  default_kscan: kscan {
    compatible = "zmk,kscan-gpio-direct";
    label = "default_kscan";

    input-gpios =  
      <&xiao_d 8 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>,
      <&xiao_d 10 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>;
  };
};